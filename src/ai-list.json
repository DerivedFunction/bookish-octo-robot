  "prompts": [
    {
      "id": "Solve",
      "prompt": "Solve the following problem and give step-by-step logic: ",
      "image": "/assets/images/prompts/solve.svg",
      "suggestions": [
        "Give a Quick Answer",
        "Check My Answer",
        "Fill in the Blanks"
      ],
      "replace": true,
      "context": ["selection"]
    },
    {
      "id": "Research",
      "prompt": "Research and compare, give the best explanation or idea for this topic: ",
      "image": "/assets/images/prompts/research.svg",
      "suggestions": [
        "Compare Options",
        "Explain This in Detail",
        "Find Latest Info"
      ],
      "replace": true,
      "context": ["selection", "page"]
    },
    {
      "id": "Summarize",
      "prompt": "Summarize briefly and concisely, with bullet points on key terms: ",
      "image": "/assets/images/prompts/summarize.svg",
